{
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
           ],
           "no-unused-vars":"off",
           "trailingComma" : "off",
           "import/prefer-default-export": "off",
           "object-curly-newline": ["error", {"multiline": true}],
           "ignoreEOLComments": true
    }
}
